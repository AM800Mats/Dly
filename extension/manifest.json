{
  "manifest_version": 3,
  "name": "Dly Chrome Extension",
  "description": "Chrome extension to fetch scores for Dly website",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.example.com/*", "https://www.example.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
